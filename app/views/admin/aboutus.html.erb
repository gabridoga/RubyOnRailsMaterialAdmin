<%= render 'admin/header' %>


<div class="row">
  <span>About Details</span>
  <form class="col s12">

    <div class="row">
      <div class="input-field col s6">
        <i class="material-icons prefix">mode_edit</i>
        <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
        <label for="icon_prefix2">Details_service</label>
      </div>
      <row>
        <div class="input-field col s6">
          <i class="material-icons prefix">account_box</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">User Experiances</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">devices</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Web and Mobile Design</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">border_color</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Programming</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">contact_phone</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Interest</label>
        </div>
      </row>
      <button class="btn waves-effect waves-light right" type="submit" name="action">Save
        <i class="material-icons right">send</i>
      </button>
    </div>




  </form>
</div>


<%= render 'admin/footer' %>